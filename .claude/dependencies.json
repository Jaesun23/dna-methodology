{
  "plugins": [
    {
      "name": "spark-agents",
      "version": "^4.3.0",
      "repository": "https://github.com/Jaesun23/spark-claude",
      "description": "SPARK Agent System - Stage 5-9 에이전트 원본 출처",
      "required_agents": [
        "implementer-spark",
        "tester-spark",
        "documenter-spark",
        "designer-spark",
        "analyzer-spark",
        "qc-spark"
      ],
      "usage": {
        "stage5": "implementer-dna (복사 from implementer-spark)",
        "stage6": "documenter-dna (복사 from documenter-spark)",
        "stage7": "designer-dna (복사 from designer-spark)",
        "stage8": "analyzer-dna (복사 from analyzer-spark)",
        "stage9": "qc-dna (복사 from qc-spark)"
      },
      "note": "Stage 5-9의 DNA 에이전트는 spark-claude 에이전트를 복사하여 이름만 변경한 것입니다."
    }
  ],
  "installation": {
    "instructions": [
      "1. SPARK 플러그인 설치: /plugin install https://github.com/Jaesun23/spark-claude",
      "2. DNA 플러그인 설치: /plugin install .",
      "3. 사용: /dna:init 으로 프로젝트 시작"
    ]
  }
}
